{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory adı",
			"defaultValue": "DataFactoryng1"
		},
		"ls_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "'ls_AzureDataLakeStorage' öğesine ait 'accountKey' için güvenli dize"
		},
		"ls_AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "'ls_AzureSqlDatabase' öğesine ait 'connectionString' için güvenli dize"
		},
		"ls_AzureSynapseAnalytics_connectionString": {
			"type": "secureString",
			"metadata": "'ls_AzureSynapseAnalytics' öğesine ait 'connectionString' için güvenli dize"
		},
		"ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrollnazg.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "Bağlı ARM şablonlarını içeren depolama hesabına yönelik URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Bağlı ARM şablonlarını içeren depolama hesabına yönelik SAS belirteci"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFactoryng1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage_accountKey')]"
					},
					"ls_AzureSqlDatabase_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase_connectionString')]"
					},
					"ls_AzureSynapseAnalytics_connectionString": {
						"value": "[parameters('ls_AzureSynapseAnalytics_connectionString')]"
					},
					"ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DataFactoryng1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DataFactoryng1_ArmTemplate_0')]"
			]
		}
	]
}